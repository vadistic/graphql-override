{
  "name": "graphql-example-example-prisma-ts",
  "scripts": {
    "start": "dotenv -- nodemon -e ts,graphql -x ts-node src/index.ts",
    "dev": "npm-run-all --parallel start playground",
    "debug": "dotenv -- nodemon -e ts,graphql -x ts-node --inspect src/index.ts",
    "playground": "graphql playground",
    "build": "rimraf dist && tsc"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "graphql-yoga": "1.17.0",
    "jsonwebtoken": "8.4.0",
    "prisma-binding": "2.2.14"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "dotenv-cli": "1.4.0",
    "graphql-cli": "3.0.6",
    "graphql-override": "0.1.0",
    "nodemon": "1.18.9",
    "npm-run-all": "4.1.5",
    "prisma": "1.25.0",
    "rimraf": "2.6.3",
    "ts-node": "8.0.1",
    "typescript": "3.2.4"
  }
}
